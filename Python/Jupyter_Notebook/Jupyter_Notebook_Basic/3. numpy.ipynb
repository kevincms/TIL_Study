   "source": [
    "# numpy를 사용하는 이유- 속도차이\r\n",
    "import numpy as np # numpy 모듈을 불러옴 - 주로 행렬이나 다차원 배열을 처리하기 위해 쓰임\r\n",
    "\r\n",
    "count=1\r\n",
    "array=np.array([1,2,3]) # list, tuple, set 등을 행렬로 바꿔줌\r\n",
    "print(\"{}. {} {}\".format(count, array,type(array)))\r\n",
    "count+=1\r\n",
    "\r\n",
    "print(\"{}. {}\".format(count, array.shape)) # 행렬의 크기 반환\r\n",
    "count+=1"
   ],
